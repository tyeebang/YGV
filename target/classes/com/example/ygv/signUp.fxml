<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="350.0" styleClass="tap" stylesheets="@main.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ygv.LogSignController">
   <children>
      <Label layoutX="103.0" layoutY="64.0" text="회원가입" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="signId" layoutX="95.0" layoutY="150.0" promptText="아이디를 입력하시오" />
      <PasswordField fx:id="signPw" layoutX="95.0" layoutY="205.0" promptText="비밀번호를 입력하시오" />
      <PasswordField fx:id="signPwCheck" layoutX="95.0" layoutY="260.0" promptText="비밀번호 확인" />
      <TextField fx:id="signName" layoutX="95.0" layoutY="315.0" promptText="이름을 입력하시오" />
      <Button fx:id="signCloseBut" layoutX="92.0" layoutY="394.0" mnemonicParsing="false" onAction="#signUpClose" styleClass="buttons" text="돌아가기" />
      <Button fx:id="insertBut" layoutX="197.0" layoutY="394.0" mnemonicParsing="false" onAction="#insertMember" styleClass="buttons" text="회원가입" />
   </children>
</AnchorPane>
